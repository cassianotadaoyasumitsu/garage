<%= link_to "New", new_user_post_path %>

<% @posts.each do |post| %>
    <p><%= post.title %></p>
    <p><%= post.description %></p>
    <p><%= post.price %></p>
    <p><%= post.year %></p>
    <p><%= post.km %></p>
    <p><%= post.post_status %></p>
    <p><%= post.user.email %></p>
    <%= link_to "Show", user_post_path(current_user, post) %>
    <%= link_to "Edit", edit_user_post_path(current_user, post) %>
    <% end %>